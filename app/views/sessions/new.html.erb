<h1>Sign In</h1>

<%= form_with(url: session_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, autofocus: true, required: true %>

  <%= f.label :password %>
  <%= f.password_field :password, required: true %>

  <%= f.submit "Sign In", class: "btn btn-primary"%>
<% end %>